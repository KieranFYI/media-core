{
    "name": "kieranfyi/media-core",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "KieranFYI\\Media\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KieranFYI\\Tests\\Media\\Core\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Kieran",
            "email": "kieran@andersons.bz"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "KieranFYI\\Media\\Core\\Providers\\MediaCorePackageServiceProvider"
            ],
            "aliases": {
                "MimeTypes": "KieranFYI\\Media\\Core\\Facades"
            }
        }
    },
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.19",
        "kieranfyi/misc": "^0.3.15",
        "kieranfyi/logging": "^0.3.13",
        "kieranfyi/services-core": "^0.0.4",
        "kieranfyi/roles-core": "^1.0.15",
        "ralouphie/mimey": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.19"
    }
}
